  		<div class="row-fluid">
		  <div class="span10 offset1"></div>
		</div>

		<div class="row-fluid">
		  <div class="span5 offset1">
		  	<%= image_tag("logo.jpg", :alt => "rss feed") %>
		  </div>	
		  <div class="span4 offset1"> 

		  	<div class="row-fluid">
		  		<div class="span10 offset1"></div>
			</div>
			<div class="row-fluid">
		  		<div class="span10 offset1"></div>
			</div>

		  	<div class="navbar">
		      <div class="navbar-inner">
