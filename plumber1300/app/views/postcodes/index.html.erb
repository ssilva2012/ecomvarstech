<h1>Listing postcodes</h1>

<table>
  <tr>
    <th>Postcode</th>
    <th>Suburb</th>
    <th>Price</th>
    <th>Isavailable</th>
    <th>Maxlimit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @postcodes.each do |postcode| %>
  <tr>
    <td><%= postcode.postcode %></td>
    <td><%= postcode.suburb %></td>
    <td><%= postcode.price %></td>
    <td><%= postcode.isAvailable %></td>
    <td><%= postcode.maxLimit %></td>
    <td><%= link_to 'Show', postcode %></td>
    <td><%= link_to 'Edit', edit_postcode_path(postcode) %></td>
    <td><%= link_to 'Destroy', postcode, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Postcode', new_postcode_path %>
