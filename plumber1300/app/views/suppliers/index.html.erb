<h1>Listing suppliers</h1>

<table>
  <tr>
    <th>Suppliercode</th>
    <th>Name</th>
    <th>Enabled</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>Email</th>
    <th>Suburb</th>
    <th>Postcode</th>
    <th>Regno</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @suppliers.each do |supplier| %>
  <tr>
    <td><%= supplier.supplierCode %></td>
    <td><%= supplier.name %></td>
    <td><%= supplier.enabled %></td>
    <td><%= supplier.address1 %></td>
    <td><%= supplier.address2 %></td>
    <td><%= supplier.email %></td>
    <td><%= supplier.suburb %></td>
    <td><%= supplier.postCode %></td>
    <td><%= supplier.regNo %></td>
    <td><%= link_to 'Edit', edit_supplier_path(supplier) %></td>
    <td><%= link_to 'Destroy', supplier, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Supplier', admin_suppliers_new_path %>
